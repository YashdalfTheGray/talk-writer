  "types": "dist/index.d.ts",
  "bin": {
    "talk-writer": "./dist/bin.js"
  },
  "scripts": {
    "prepare": "npm run build",
    "prebuild": "npx rimraf dist",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YashdalfTheGray/talk-writer.git"
  },
  "author": "Yash Kulshrestha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YashdalfTheGray/talk-writer/issues"
  },
  "homepage": "https://github.com/YashdalfTheGray/talk-writer#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
