    "@types/webpack-merge": "^4.1.5",
    "@types/yargs": "^13.0.0",
    "babel-loader": "^8.0.6",
    "chalk": "^2.4.2",
    "core-js": "^3.1.4",
    "css-loader": "^3.0.0",
    "file-loader": "^4.0.0",
    "html-loader": "^0.5.5",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0",
    "markdown-loader": "^5.0.0",
    "prettier": "^1.18.2",
